				},
				{
					"colors": [
						"#ffffff",
						"#ffffff00"
					],
					"name": "Background and Transparent",
					"slug": "background-and-transparent"
				},
				{
					"colors": [
						"#ffffff00",
						"#d20394"
					],
					"name": "Transparent and Primary",
