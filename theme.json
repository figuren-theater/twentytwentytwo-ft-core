						"#e89700",
						"#3e58e1"
					],
					"name": "Primary-3rd and Primary-2nd",
					"slug": "primary-3rd-and-primary-2nd"
				},
				{
					"colors": [
						"#3e58e1",
						"#e89700"
					],
					"name": "Primary-2nd and Primary-3rd",
					"slug": "primary-2nd-and-primary-3rd"
				},
				{
					"colors": [
						"#ffffff00",
						"#e89700"
					],
					"name": "Transparent and Primary-3rd",
					"slug": "transparent-and-primary-3rd"
				},
				{
					"colors": [
						"#e89700",
						"#ffffff00"
					],
					"name": "Primary-3rd and Transparent",
					"slug": "primary-3rd-and-transparent"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(to bottom,var(--wp--preset--color--secondary) 0%,var(--wp--preset--color--tertiary) 100%)",
					"name": "Vertical Secondary to Tertiary",
					"slug": "vertical-secondary-to-tertiary"
				},
				{
					"gradient": "linear-gradient(to bottom,var(--wp--preset--color--secondary) 0%,var(--wp--preset--color--background) 100%)",
					"name": "Vertical Secondary to Background",
					"slug": "vertical-secondary-to-background"
				},
				{
					"gradient": "linear-gradient(to bottom,var(--wp--preset--color--tertiary) 0%,var(--wp--preset--color--background) 100%)",
					"name": "Vertical Tertiary to Background",
					"slug": "vertical-tertiary-to-background"
				},
				{
					"gradient": "linear-gradient(to bottom right,var(--wp--preset--color--primary) 0%,var(--wp--preset--color--foreground) 100%)",
					"name": "Diagonal Primary to Foreground",
					"slug": "diagonal-primary-to-foreground"
				},
				{
					"gradient": "linear-gradient(to bottom right,var(--wp--preset--color--secondary) 50%,var(--wp--preset--color--background) 50%)",
					"name": "Diagonal Secondary to Background",
					"slug": "diagonal-secondary-to-background"
				},
				{
					"gradient": "linear-gradient(to bottom right,var(--wp--preset--color--background) 50%,var(--wp--preset--color--secondary) 50%)",
					"name": "Diagonal Background to Secondary",
					"slug": "diagonal-background-to-secondary"
				},
				{
					"gradient": "linear-gradient(to bottom right,var(--wp--preset--color--tertiary) 50%,var(--wp--preset--color--background) 50%)",
					"name": "Diagonal Tertiary to Background",
					"slug": "diagonal-tertiary-to-background"
				},
				{
					"gradient": "linear-gradient(to bottom right,var(--wp--preset--color--background) 50%,var(--wp--preset--color--tertiary) 50%)",
					"name": "Diagonal Background to Tertiary",
					"slug": "diagonal-background-to-tertiary"
				},
				{
					"gradient": "radial-gradient(var(--wp--preset--color--primary) 0%,#ffffff00 39%)",
					"name": "Radial Transparent to Primary",
					"slug": "radial-transparent-to-primary"
				},
				{
					"gradient": "radial-gradient(var(--wp--preset--color--primary-2nd) 0%,#ffffff00 39%)",
					"name": "Radial Transparent to Primary-2nd",
					"slug": "radial-transparent-to-primary-2nd"
				},
				{
					"gradient": "radial-gradient(var(--wp--preset--color--primary-3rd) 0%,#ffffff00 39%)",
					"name": "Radial Transparent to Primary-3rd",
					"slug": "radial-transparent-to-primary-3rd"
				}
			],
			"palette": [
				{
					"color": "#d20394",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#3e58e1",
					"name": "Primary-2nd",
					"slug": "primary-2nd"
				},
				{
					"color": "#e89700",
					"name": "Primary-3rd",
					"slug": "primary-3rd"
				},
				{
					"color": "#0f0b0e",
					"name": "Foreground",
					"slug": "foreground"
				},
				{
					"color": "#181617",
					"name": "Foreground (90%)",
					"slug": "foreground-decent"
				},
				{
					"color": "#50494d",
					"name": "Foreground (70%)",
					"slug": "foreground-dimmed"
				},
				{
					"color": "#c7c7d1",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#efefec",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#fbf9fa",
					"name": "Background",
					"slug": "background"
				},
				{
					"color": "#5e801b",
					"name": "sustainability",
					"slug": "custom-sustainability"
				}
			],
			"text": true
		},
		"custom": {
			"border": {
				"radius": "17px"
			},
			"spacing": {
				"large": "clamp(4rem, 10vw, 8rem)",
				"medium": "clamp(2rem, 8vw, calc(4 * var(--wp--style--block-gap)))",
				"outer": "var(--wp--custom--spacing--small, 1.25rem)",
				"small": "max(1.25rem, 5vw)"
			},
			"typography": {
				"font-size": {
					"colossal": "clamp(3.25rem, 8vw, 6.25rem)",
					"gigantic": "clamp(2.75rem, 6vw, 3.25rem)",
					"huge": "clamp(2.25rem, 4vw, 2.75rem)"
				},
				"fontSize": {
					"colossal": "clamp(3.25rem, 8vw, 6.25rem)",
					"gigantic": "calc(2.4 * var(--wp--custom--typography--font-size--normal))",
					"huge": "clamp(2.25rem, 4vw, 2.75rem)",
					"normal": "clamp(0.8rem, 0.5vw + 1rem, 1.5rem);"
				},
				"line-height": {
					"medium": 1.4,
					"normal": 1.6,
					"small": 1.2,
					"tiny": 1.15
				},
				"lineHeight": {
					"medium": 1.4,
					"normal": 1.6,
					"small": 1.2,
					"tiny": 1.15
				}
			}
		},
		"layout": {
			"contentSize": "1050px",
			"wideSize": "1300px"
		},
		"spacing": {
			"units": [
				"em",
				"rem",
				"px",
				"%",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": false,
			"dropCap": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "\"Source Serif Pro\", serif",
					"name": "Source Serif Pro",
					"slug": "source-serif-pro"
				},
				{
					"fontFamily": "Open Sans, sans-serif",
					"name": "Open Sans",
					"slug": "open-sans"
				}
			],
			"fontSizes": [
				{
					"size": "clamp(.55rem, 0.5vw + 1rem, 1rem)",
					"slug": "small"
				},
				{
					"size": "clamp(0.7rem, 0.5vw + 1rem, 1.25rem)",
					"slug": "medium"
				},
				{
					"size": "clamp(1.5rem, 2.5vw, 2rem)",
					"slug": "large"
				},
				{
					"size": "clamp(1.75rem, 3vw, 2.25rem)",
					"slug": "x-large"
				}
			],
			"fontStyle": true,
			"fontWeight": false,
			"letterSpacing": false,
			"lineHeight": false,
			"textDecoration": true,
			"textTransform": true
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"color": "currentColor",
					"radius": "var(--wp--custom--border--radius)"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--background)"
				},
				"spacing": {
					"padding": ".5em 1em"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/post-comments": {
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--spacing--small)"
					}
				}
			},
			"core/post-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--source-serif-pro)",
					"fontSize": "var(--wp--custom--typography--font-size--gigantic)",
					"fontWeight": "300",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"core/pullquote": {
				"border": {
					"width": "1px 0"
				}
			},
			"core/query-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--source-serif-pro)",
					"fontSize": "var(--wp--custom--typography--font-size--gigantic)",
					"fontWeight": "300",
					"lineHeight": "var(--wp--custom--typography--line-height--small)"
				}
			},
			"core/quote": {
				"border": {
					"width": "1px"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-font)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontStyle": "italic",
					"fontWeight": "normal",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)"
				}
			}
		},
		"border": {
			"color": "var(--wp--preset--color--primary)",
			"radius": "var(--wp--custom--border--radius)",
			"width": "var(--wp--custom--spacing--small)"
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--source-serif-pro)",
					"fontSize": "var(--wp--custom--typography--font-size--colossal)",
					"fontWeight": "300",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--source-serif-pro)",
					"fontSize": "var(--wp--custom--typography--font-size--gigantic)",
					"fontWeight": "300",
					"lineHeight": "var(--wp--custom--typography--line-height--small)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--source-serif-pro)",
					"fontSize": "var(--wp--custom--typography--font-size--huge)",
					"fontWeight": "300",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--source-serif-pro)",
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "300",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--source-serif-pro)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--source-serif-pro)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"textTransform": "uppercase"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--open-sans)",
			"fontSize": "var(--wp--custom--typography--font-size--normal)",
			"lineHeight": "var(--wp--custom--typography--line-height--normal)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "header",
			"name": "header-large-dark",
			"title": "Header (dunkel, gro√ü)"
		},
		{
			"area": "header",
			"name": "header-small-dark",
			"title": "Header (dunkel, klein)"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"version": 2
}
