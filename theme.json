{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"customTemplates": [
		{
			"name": "blank",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Blank"
		},
		{
			"name": "single-with-separators",
			"postTypes": [
				"post"
			],
			"title": "Beitrag (Mit Trenner)"
		},
		{
			"name": "page-with-separators",
			"postTypes": [
				"page"
			],
			"title": "Seite (Mit Trenner)"
		},
		{
			"name": "page-no-title",
			"postTypes": [
				"page",
				"post",
				"ft_production",
				"ft_theme",
				"tb_prod_subsite"
			],
			"title": "Seite (Ohne Seitentitel)"
		},
		{
			"name": "page-large-header",
			"postTypes": [
				"page"
			],
			"title": "Seite (Gro√üer Kopfteil)"
		}
	],
	"settings": {
		"useRootPaddingAwareAlignments": true,
		"appearanceTools": true,
		"blocks": {
			"core/button": {
				"border": {
					"color": true,
					"radius": true,
					"style": true,
					"width": true
				},
				"typography": {
					"textDecoration": true
				}
			},
			"core/group": {
				"border": {
					"color": true,
					"radius": true,
					"style": true,
					"width": true
				}
			}
		},
		"color": {
			"background": true,
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [
				{
					"colors": [
						"#ffffff",
						"#000000"
					],
